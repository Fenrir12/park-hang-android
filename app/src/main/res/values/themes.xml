<?xml version="1.0" encoding="utf-8"?>
<resources xmlns:tools="http://schemas.android.com/tools">
    <style name="Theme.ParkHang" parent="android:Theme.Material.Light.NoActionBar">
        <item name="android:background">@color/transparent_background</item>
        <item name="android:windowIsTranslucent">true</item>
    </style>
    <style name="Theme.ParkHang.Splashscreen" parent="Theme.SplashScreen">
        <item name="windowSplashScreenAnimatedIcon">@drawable/logo_park_hang</item>
        <item name="windowSplashScreenBackground">@color/splashscreen_background</item>
        <item name="postSplashScreenTheme">@style/Theme.ParkHang</item>
        <item name="android:windowSplashScreenBehavior" tools:targetApi="33">icon_preferred</item>
    </style>
</resources>
